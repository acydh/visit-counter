<% include ../partials/header %>
  <div class="container">
    <h2>Website: <%= website %> <span class="key-span">(<%= key %>)</span></h2>
    <h2>Visits: <%= visits %></h2>
    <h2>Uniques: <%= uniques %></h2>
    <h4>Log:</h4>
    <ul>
    <% dataCollection.forEach(function(ip) { %>
        <li><%=('0'+ip.dateStamp.getDate()).slice(-2)%>/<%=('0'+(ip.dateStamp.getMonth()+1)).slice(-2)%>/<%=ip.dateStamp.getFullYear()%> <%=ip.dateStamp.getHours()%>.<%=('0'+ip.dateStamp.getMinutes()).slice(-2)%>.<%=('0'+ip.dateStamp.getSeconds()).slice(-2) %> - <%= ip.ip %> - <%= ip.hash %> - <%= ip.country %> - <%= ip.city %></li>
    <% }); %>
    </ul>

  </div>

<% include ../partials/footer %>
